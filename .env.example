# Cloudflare Tunnel URL to your server machine
TUNNEL_URL=https://your-tunnel-domain.example.com

# Shared auth token (must match cc-server/.env)
SHARED_SECRET=your-shared-secret-here

# NextAuth
NEXTAUTH_SECRET=your-nextauth-secret-here
NEXTAUTH_URL=https://your-app.vercel.app

# Google OAuth
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Comma-separated list of allowed Google emails
ALLOWED_EMAILS=your-email@example.com
